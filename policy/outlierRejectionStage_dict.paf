#<?cfg paf dictionary ?>

target: lsst.coadd.pipeline.OutlierRejectionStage

definitions: {
    inputKeys: {
        description: "Names of input items on the clipboard."
        type: "policy"
        dictionary: {
            definitions: {
                exposureList: {
                    description: "List of psf-matched intensity-matched exposures
                        (std::vector<afwImage.Exposure<x>::Ptr>)."
                    type: "string"
                    minOccurs: 1
                    maxOccurs: 1
                    default: "exposureList"
                }        
            }
        }
        minOccurs: 1
        maxOccurs: 1
    }
    
    #output clipboard keys:
    outputKeys: {
        description: "Names if output items on the clipboard."
        type: "policy"
        dictionary: {
            definitions: {
                coadd: {
                    description: "Coadd: mean of input images with outlier rejection (afwImage.Exposure<x>)."
                    type: "string"
                    minOccurs: 1
                    maxOccurs: 1
                    default: "coadd"
                }        
                weightMap: {
                    description: "Coadd weight map (afwImage.ImageF).
                        Only output if the event's isLastExposure is True."
                    type: "string"
                    minOccurs: 1
                    maxOccurs: 1
                    default: "weightMap"
                }
            }
        }
        minOccurs: 1
        maxOccurs: 1
    }

    outlierRejectionPolicy: {
        description: "Policy to control outlier rejection."
        type: "policy"
        dictionary: @outlierRejection_dict.paf
        minOccurs: 1
        maxOccurs: 1
    }
}
